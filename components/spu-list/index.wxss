.spu_listWarp{
  padding: 0 42rpx;
  background: #fff;
  overflow: hidden;
}
.spu_cardWarp{
  height: 456rpx;
  width: 322rpx;
  padding-top: 40rpx;
}
.spu_cardWarp .spu_pic{
  width: 322rpx;
  height: 322rpx;
}
.spu_cardWarp .spu_pic image{
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 24rpx;
}
.spu_cardWarp .spu_name{
  height: 36rpx;
  font-size: 30rpx;
  line-height: 36rpx;
  margin-bottom: 12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: "pingFang SC Regular"
}
.spu_cardWarp .spu_cash{
  height: 24rpx;
  line-height: 124rpx2px;
  color: #004c20;
  font-size: 24rpx;
  font-family: "pingFang SC Medium";
  margin-bottom: 12rpx;
  font-weight: bold;
}
.spu_cardWarp .spu_cash text:nth-child(1){
  font-size: 20rpx;
  width: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
  margin-right: 6rpx;
}
.spu_cardWarp .spu_cash text:nth-child(2){
  margin-right: 10rpx
}
.spu_cardWarp .spu_point{
  height: 24rpx;
  line-height: 124rpx2px;
  color: #004c20;
  font-size: 24rpx;
  font-family: "pingFang SC Medium";
  margin-bottom: 12rpx;
  font-weight: bold;
}
.spu_cardWarp .spu_point text:nth-child(1){
  font-size: 20rpx;
  width: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
  margin-right: 6rpx;
}
.spu_cardWarp .spu_point text:nth-child(2){
  margin-right: 10rpx
}